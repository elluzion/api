    "@elysiajs/eden": "^1.1.2",
    "@elysiajs/swagger": "^1.1.0",
    "@types/bun": "^1.1.6",
    "elysia": "latest",
    "soundcloud.ts": "0.5.2"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "module": "src/index.js"
}
